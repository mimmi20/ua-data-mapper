{
  "name": "mimmi20/ua-data-mapper",
  "description": "a class to map the results of different useragent parsers",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-data-mapper/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-data-mapper",
  "support": {
    "issues": "https://github.com/mimmi20/ua-data-mapper/issues",
    "source": "https://github.com/mimmi20/ua-data-mapper"
  },
  "require": {
    "php": "~8.4.0 || ~8.5.0",
    "ext-mbstring": "*",
    "mimmi20/browser-detector-version": "^13.0.0",
    "mimmi20/browser-detector-versioninterface": "^1.0.0",
    "mimmi20/ua-browser-type": "^12.0.1",
    "mimmi20/ua-device-type": "^12.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "filp/whoops": "^2.18.4",
    "infection/infection": "^0.31.9",
    "mimmi20/coding-standard": "^7.0.7",
    "nikic/php-parser": "^5.6.2",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.32",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpstan/phpstan-phpunit": "^2.0.8",
    "phpunit/phpunit": "^12.4.3",
    "rector/rector": "^2.2.8",
    "rector/type-perfect": "^2.1.1",
    "shipmonk/composer-dependency-analyser": "^1.8.3",
    "symfony/process": "^7.3.4",
    "symplify/phpstan-rules": "^14.8.3",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.1.1",
    "vimeo/psalm": "^6.13.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UaDataMapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaDataMapperTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
